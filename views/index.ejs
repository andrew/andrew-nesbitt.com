<section>
<h2>Upcoming Events</h2>
<ul>
<% for (var e = 0; e < events.length; e++) {  %>
  <% var event = events[e] %>
  <li>
    <h3><%= event.title %><a href='<%= event.external %>' target='_blank'><%= event.subtitle %></a></h3>
    <p></p>
  </li>
<% } %>
</ul>
</section>